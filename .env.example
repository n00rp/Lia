# Exempel på miljövariabler för Simulink WebView Navigation System

# Nätverkssökväg till System_Releases
NETWORK_PATH=\\server\System_Releases

# Lokal testsökväg (används om USE_NETWORK=False)
LOCAL_TEST_PATH=C:\TestData\System_Releases

# Använd nätverkssökväg (True) eller lokal sökväg (False)
USE_NETWORK=True

# Flask debug-läge
DEBUG=True

# Server host och port
HOST=0.0.0.0
PORT=5000

# Flask miljö (development/production)
FLASK_ENV=development
